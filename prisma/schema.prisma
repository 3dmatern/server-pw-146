generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model auth {
  userid Int @default(0)
  zoneid Int @default(0)
  rid    Int @default(0)

  @@id([userid, zoneid, rid])
}

model forbid {
  userid      Int      @default(0)
  type        Int      @default(0)
  ctime       DateTime @db.DateTime(0)
  forbid_time Int      @default(0)
  reason      Bytes    @db.Blob
  gmroleid    Int?     @default(0)

  @@id([userid, type])
}

model iplimit {
  uid        Int     @id @default(0)
  ipaddr1    Int?    @default(0)
  ipmask1    String? @default("") @db.VarChar(2)
  ipaddr2    Int?    @default(0)
  ipmask2    String? @default("") @db.VarChar(2)
  ipaddr3    Int?    @default(0)
  ipmask3    String? @default("") @db.VarChar(2)
  enable     String? @default("") @db.Char(1)
  lockstatus String? @default("") @db.Char(1)
}

model point {
  uid          Int       @default(0)
  aid          Int       @default(0)
  time         Int       @default(0)
  zoneid       Int?      @default(0)
  zonelocalid  Int?      @default(0)
  accountstart DateTime? @db.DateTime(0)
  lastlogin    DateTime? @db.DateTime(0)
  enddate      DateTime? @db.DateTime(0)

  @@id([uid, aid])
  @@index([aid, zoneid], map: "IX_point_aidzoneid")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model usecashlog {
  userid   Int      @default(0)
  zoneid   Int      @default(0)
  sn       Int      @default(0)
  aid      Int      @default(0)
  point    Int      @default(0)
  cash     Int      @default(0)
  status   Int      @default(0)
  creatime DateTime @db.DateTime(0)
  fintime  DateTime @db.DateTime(0)

  @@index([creatime], map: "IX_usecashlog_creatime")
  @@index([userid, zoneid, sn], map: "IX_usecashlog_uzs")
  @@ignore
}

model usecashnow {
  userid   Int      @default(0)
  zoneid   Int      @default(0)
  sn       Int      @default(0)
  aid      Int      @default(0)
  point    Int      @default(0)
  cash     Int      @default(0)
  status   Int      @default(0)
  creatime DateTime @db.DateTime(0)

  @@id([userid, zoneid, sn])
  @@index([creatime], map: "IX_usecashnow_creatime")
  @@index([status], map: "IX_usecashnow_status")
}

model users {
  ID           Int       @id @default(0)
  name         String    @unique(map: "IX_users_name") @default("") @db.VarChar(32)
  passwd       String    @db.VarChar(64)
  Prompt       String    @default("") @db.VarChar(32)
  answer       String    @default("") @db.VarChar(32)
  truename     String    @default("") @db.VarChar(32)
  idnumber     String    @default("") @db.VarChar(32)
  email        String    @default("") @db.VarChar(64)
  mobilenumber String?   @default("") @db.VarChar(32)
  province     String?   @default("") @db.VarChar(32)
  city         String?   @default("") @db.VarChar(32)
  phonenumber  String?   @default("") @db.VarChar(32)
  address      String?   @default("") @db.VarChar(64)
  postalcode   String?   @default("") @db.VarChar(8)
  gender       Int?      @default(0)
  birthday     DateTime? @db.DateTime(0)
  creatime     DateTime  @db.DateTime(0)
  qq           String?   @default("") @db.VarChar(32)
  passwd2      String?   @db.VarChar(64)

  @@index([creatime], map: "IX_users_creatime")
}
